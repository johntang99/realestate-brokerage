{
  "name": "multi-site-content-platform",
  "version": "1.0.0",
  "description": "Multi-site content management template",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3040",
    "dev:default": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "ci:check": "npm run type-check && npm run lint && npm run build",
    "media:migrate": "node scripts/migrate-media-to-supabase.mjs",
    "content:normalize-media-urls": "node scripts/normalize-content-media-urls.mjs",
    "seed": "node scripts/seed-julia-content.mjs",
    "qa:routes": "node scripts/qa/check-routes.mjs",
    "qa:seo": "node scripts/qa/check-seo.mjs",
    "qa:content": "node scripts/qa/check-content.mjs",
    "qa:bilingual": "node scripts/qa/check-bilingual.mjs",
    "qa:all": "npm run qa:content && npm run qa:bilingual && npm run qa:routes && npm run qa:seo"
  },
  "dependencies": {
    "@fullcalendar/daygrid": "^6.1.20",
    "@fullcalendar/interaction": "^6.1.20",
    "@fullcalendar/react": "^6.1.20",
    "@fullcalendar/timegrid": "^6.1.20",
    "@hookform/resolvers": "^5.2.2",
    "@supabase/supabase-js": "^2.95.3",
    "bcryptjs": "^3.0.3",
    "clsx": "^2.1.0",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.312.0",
    "negotiator": "^0.6.3",
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.4.0",
    "react-hook-form": "^7.71.1",
    "react-markdown": "^10.1.0",
    "resend": "^3.0.0",
    "sharp": "^0.34.5",
    "twilio": "^5.12.1",
    "uuid": "^13.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/negotiator": "^0.6.3",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  }
}
